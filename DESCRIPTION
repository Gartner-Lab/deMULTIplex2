    MASS,
    RColorBrewer,
    ggplot2,
    grid,
    gridExtra,
    viridis,
    ggExtra,
    stringdist,
    ShortRead,
    data.table,
    uwot,
    XVector,
    devtools
Suggests:
    ggrastr
License: CC BY 4.0
Encoding: UTF-8
LazyLoad: yes
LazyData: true
biocViews: Software, Sequencing, RNASeq
